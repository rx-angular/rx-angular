    "state": {
      "projectType": "library",
      "root": "libs/state",
      "sourceRoot": "libs/state/src",
      "prefix": "rx",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "options": {
