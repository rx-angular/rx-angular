{
  "retryOn429": true,
  "httpHeaders": [
    {
      "urls": ["https://ngrx.io/", "https://www.reddit.com"],
      "headers": {
        "Accept-Encoding": "zstd, br, gzip, deflate"
      }
    }
  ],
  "retryCount": 2,
  "fallbackRetryDelay": "1s",
  "ignorePatterns": [
    {
      "pattern": "^https://ngrx.io/"
    }
  ]
}
