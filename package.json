    "jest": "27.3.1",
    "jest-preset-angular": "11.0.0-rc.3",
    "klaw-sync": "^6.0.0",
    "lint-staged": "^12.0.3",
    "ng-morph": "^2.0.0",
    "ng-packagr": "12.2.7",
    "postcss": "^8.4.6",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^10.1.1",
    "prettier": "2.5.1",
